@inproceedings{shih2013democratizing,
author = {Shih, Fuming and Seneviratne, Oshani and Liccardi, Ilaria and Patton, Evan and Meier, Patrick and Castillo, Carlos},
title = {Democratizing mobile app development for disaster management},
year = {2013},
