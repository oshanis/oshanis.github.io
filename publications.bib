  abstract = {Our research sheds light on the intricate workings of GameFi ecosystems by analyzing The Sandbox’s transaction graph. We fit this graph to the bow-tie model and find that outside support does little to boost activity and that a dedicated user base becomes resistant to scandals in the long term. We also find that whales, i.e., users with significant holdings, are involved in most transactions and exercise major influence. Our findings hold significant implications for the future development of equitable and sustainable GameFi platforms, offering valuable insights into stakeholder behavior and network resilience in the face of external challenges and opportunities.},
  booktitle = {Companion Publication of the 16th ACM Web Science Conference},
  pages = {10–11},
